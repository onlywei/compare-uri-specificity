{
  "name": "compare-uri-specificity",
  "version": "1.0.0",
  "type": "module",
  "description": "Helper function to compare the specificity of two URIs, routes, or paths",
  "main": "dist/index.js",
  "packageManager": "pnpm@10.26.2+sha512.0e308ff2005fc7410366f154f625f6631ab2b16b1d2e70238444dd6ae9d630a8482d92a451144debc492416896ed16f7b114a86ec68b8404b2443869e68ffda6",
  "repository": {
    "type": "git",
    "url": "https://github.com/onlywei/compare-uri-specificity.git"
  },
  "scripts": {
    "build": "tsc",
    "check-exports": "attw --pack . --ignore-rules=cjs-resolves-to-esm",
    "check-format": "biome check .",
    "ci": "pnpm build && pnpm check-format && pnpm check-exports && pnpm test",
    "format": "biome check --write .",
    "prepublishOnly": "pnpm run ci",
    "test": "node --test"
  },
  "keywords": [
    "compare",
    "matcher",
    "path",
    "pattern",
    "priority",
    "route",
    "routing",
    "specificity",
    "uri",
    "url"
  ],
  "author": "Wei Wang <wei@fastmail.com>",
  "license": "MIT",
  "files": [
    "dist",
    "!dist/*.test.*"
  ],
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.18.2",
    "@biomejs/biome": "^2.3.10",
    "@changesets/cli": "^2.29.8",
    "@types/node": "^24.10.4",
    "typescript": "^5.9.3"
  }
}
